{
  "name": "@kobbleio/next",
  "version": "1.3.1",
  "description": "Next SDK for Kobble",
  "scripts": {
    "build": "npm run build:swc && pnpm types",
    "build:swc": "swc src -d dist --copy-files",
    "dev": "npm run build:swc -w",
    "types": "tsc --emitDeclarationOnly",
    "lint": "TIMING=1 eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf dist && rm -rf .turbo",
    "buildpack": "npm run build && npm pack",
    "buildpublish": "npm run buildpack && npm publish --access public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client/index.js",
      "types": "./dist/client/index.d.ts"
    },
    "./server": {
      "import": "./dist/server/index.js",
      "types": "./dist/server/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "next.js",
    "kobble",
    "next authentication",
    "next permissions",
    "next ACL"
  ],
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.75",
    "@types/react-dom": "^18.2.24",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "swc": "^1.0.11",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "path-to-regexp": "^6.2.2"
  }
}
